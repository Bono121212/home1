.file-screen {
  padding: 25px 20px;
  background: #fff;
  border: 1px solid #dddddd;
  width: 100%;
}

.file-screen input[type='file'] {
  /* 파일 필드 숨기기 */
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  border: 0;
  border: 1px dashed #ddd !important;
  background: #f8f8f8;
}

.file-screen label {
  position: relative;
  display: inline-block;
  padding: .5em .75em;
  color: #999;
  font-size: inherit;
  line-height: normal;
  vertical-align: middle;
  background-color: #fff;
  cursor: pointer;
  border: 1px dashed #ddd;
  border-bottom-color: #e2e2e2;
  border-radius: 0;
  width: 130px !important;
  height: 130px !important;
  background-color: #fdfdfd;
  min-width: auto !important;
  overflow: hidden;

  &:after {
    content: '';
    display: block;
    position: absolute;
    /* background-image: url('@{static-url}/images/down/btn-photo50.svg'); */
    background-position: 0 0;
    background-position: 0 0;
    top: calc(50% - 7.5px);
    left: calc(50% - 7.5px);
    width: 15px;
    height: 15px;

  }

  &.screen-img {
    &:after {
        content: '' !important;
    }

    .ui.image {
        height: 100%;
        max-width: auto !important;
    }
  }
}

.file-screen label:hover, .file-screen label:focus {
  background: #f8f8f8 !important;
}

