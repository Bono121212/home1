        width: 2.875rem !important;
        height: 1.5rem !important;
    }

    .ui.toggle.checkbox .box:before, .ui.toggle.checkbox label:before {
        background: none !important;
    }

    .ui.toggle.checkbox .box:after, .ui.toggle.checkbox label:after {
        background: #4b4e53 !important;
        width:  2.875rem;
    }

    .ui.toggle.checkbox input:checked ~ .box:before, .ui.toggle.checkbox input:checked ~ label:before {
        opacity: 0;
    }

    .ui.checkbox input:checked ~ label:after {
        width: 2.875rem !important;
        height: 1.5rem !important;
        opacity: 1 !important;
    }
    .ui.toggle.checkbox input:checked ~ .box:after, .ui.toggle.checkbox input:checked ~ label:after {
        left: 2.5rem;
    }

    .ui.fitted.toggle.checkbox, .ui.fitted.toggle.checkbox {
        width: 5.625rem;
        height: 1.75rem;
        padding: 0.12rem 0.19rem 0.12rem 0.15rem;
        border-radius: 1.34375rem;
        background-color: #eee;
        position: relative;

        &:after {
            content:'EN';
            width: 2.875rem;
            height: 1.5rem;
            text-align: center;
            display: block;
            position: absolute;
            right: 2px;
            top: 2px;
            font-weight: bold;
            z-index: 10;
            color: #444;
            font-size: 14px;
            cursor: pointer;

        }
        &:before {
            content:'KR';
            width: 2.875rem;
            height: 1.5rem;
            text-align: center;
            display: block;
            position: absolute;
            left: 2px;
            top: 2px;
            font-weight: bold;
            z-index: 10;
            color: #eeeeee;
            font-size: 14px;
            cursor: pointer;

        }
    }


    .ui.checked.fitted.toggle.checkbox, .ui.checked.fitted.toggle.checkbox {

         &:after {
            color: #fff;
            -webkit-transition: color 150ms linear;
            -ms-transition: color 150ms linear;
            transition: color 150ms linear;
         }

         &:before {
            color: #444;
            -webkit-transition: color 150ms linear;
            -ms-transition: color 150ms linear;
            transition: color 150ms linear;
         }
    }

    .ui.checkbox input:checked ~ label:before {
        width: 2.875rem !important;
        height: 1.5rem !important;
        background-size: 100% auto !important;
        background-image: none;
        box-sizing: border-box;
    }

    .ui.toggle.checkbox .box, .ui.toggle.checkbox:after {
            color: #f00;

        label:after {
            background: #f00;
            padding: 0;
            width: 2.875rem;
            height: 1.5rem;
            font-size: 0.875rem;
            font-weight: 700;
            background: #4b4e53;
            transition: none;
            opacity: 1 !important;

        }
    }

