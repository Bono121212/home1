    -o-text-overflow: ellipsis;
       text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    overflow: hidden;
}

.g-menu-detail .layer .table-css .row.body .cell:last-of-type button span {
    margin: 0 0 0 0.25rem;
    font-size: 0.75rem;
