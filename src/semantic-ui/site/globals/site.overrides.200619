/* layout */
body {
    background: #f4f7fd;
    min-width: 77.5rem;
    overflow-x: auto;
}

#root {
    height: 100%;
}

#root .cont-inner {
    position: relative;
    width: 75rem;
    margin: 0 auto;
}

.cont-inner .ml17 {
    padding: 0.5rem 0;
}

#root section.header {
    width: 100%;
    min-width: 77.5rem;
}

#root section.header .group {
    position: relative;
    height: 4.75rem;
    background: #fff;
}

#root section.header .group .cont-inner {
    height: 100%;
}

#root section.header .group .g-logo {
    position: absolute;
    padding-top: 1.3125rem;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    width: 6.125rem;
    height: 100%;
}

#root section.header .group .g-logo i {
    vertical-align: top;
}

#root section.header .group .g-menu-detail {
    position: absolute;
    left: 8.1875rem;
    top: 0;
}

.g-menu-detail .ui.button.close::before {
        display: none;
}

#root section.header .group .g-menu-detail::before {
    content: '';
    width: 0.0625rem;
    height: 4.75rem;
    background: #eee;
    position: absolute;
    top: 0;
    left: 9.5rem;
}

#root section.header .group .g-menu-detail .detail-open {
    position: absolute;
    left: 0;
    top: 1.5625rem;
    padding: 0 1.625rem 0 2.625rem;
    height: 2rem;
    font-size: 0.75rem;
    font-weight: 700;
    color: #6c788f;
    background: #fff url('@{static-url}/images/down/icon-category-gray-16-px.svg') no-repeat 1.4375rem 0.375rem;
    border-radius: 0.25rem;
    border: 1px solid #6c788f;
}

.g-menu-detail.visible {
    padding: 0 !important;
    border: none !important;
    box-shadow: none !important;
    margin: 0.5em 0em 0em !important;
}

.g-menu-detail .layer {
    z-index: 10;
    width: 32.9375rem;
    background: #fff;
    border-radius: 0.25rem;
    -webkit-box-shadow: 0 0.0625rem 0.4375rem -0.1875rem #3a4f82;
            box-shadow: 0 0.0625rem 0.4375rem -0.1875rem #3a4f82;
            position: relative;

    .button {
        background: inherit !important;
        padding: 0;
        border-radius: 0 !important;
    }
}

.g-menu-detail .layer .table-css {
    width: 100%;
}

.g-menu-detail .layer .table-css .row.head .cell {
    width: 10rem;
    border-bottom: 0.0625rem solid #d6deed;
    font-size: 0.75rem;
    font-weight: 700;
    color: #222;
    text-indent: 1.25rem;
    height: 2.4375rem;
}

.g-menu-detail .layer .table-css .row.head .cell:last-of-type {
    width: auto;
    border-left: 0.0625rem solid #d6deed;
}

.g-menu-detail .layer .table-css .row.body .cell {
    height: 18.625rem;
}

.g-menu-detail .layer .table-css .row.body .cell:first-of-type .select-area {
    width: 100%;
    padding: 0.375rem 0;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    min-width: 160px;
}

.g-menu-detail .layer .table-css .row.body .cell:first-of-type button {
    width: 100%;
    height: 2.25rem;
    font-size: 0.875rem;
    font-weight: 700;
    text-indent: 1.25rem;
    text-align: left;
    letter-spacing: -0.0125rem;
    color: #222;
}

.g-menu-detail .layer .table-css .row.body .cell:first-of-type button:hover, .g-menu-detail .layer .table-css .row.body .cell:first-of-type button.active {
    background: #f4f7fd !important;
}

.g-menu-detail .layer .table-css .row.body .cell:last-of-type {
    border-left: 0.0625rem solid #d6deed;
}

.g-menu-detail .layer .table-css .row.body .cell:last-of-type .select-area {
    width: 100%;
    padding: 0.375rem 0.625rem 0.375rem 0;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}

.g-menu-detail .layer .table-css .row.body .cell:last-of-type .select-area .scrolling {
    padding: 0 1rem 0 0;
}

.g-menu-detail .layer .table-css .row.body .cell:last-of-type button {
    height: 2.25rem;
    font-size: 0.875rem;
    text-indent: 1.25rem;
    text-align: left;
    letter-spacing: -0.0125rem;
    color: #222;
    background: url('@{static-url}/images/down/icon-arrow-16-px.svg') no-repeat right 0.625rem !important;
    width: 20.625rem;
    padding-right: 1.875rem;
    -webkit-box-sizing: border-box;
